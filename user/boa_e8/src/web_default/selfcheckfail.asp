<script language="javascript" type="text/javascript">

function setCookie(cname, cvalue, exsec)
{
    var d = new Date();
    d.setTime(d.getTime() + (exsec*1000));
    var expires = "expires="+ d.toUTCString();
    document.cookie = cname + "=" + cvalue + "; " + expires;
}

